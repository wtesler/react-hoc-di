{
  "name": "react-hoc-di",
  "version": "1.1.3",
  "description": "Dependency-inject a module using react higher-order components (HOCs)",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "keywords": [
    "react",
    "dependency-inject",
    "hoc"
  ],
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/react-hoc-di.git"
  },
  "homepage": "https://github.com/wtesler/react-hoc-di"
}
